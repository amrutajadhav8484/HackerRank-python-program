def breakingRecords(scores):
    # Write your code here
    score=scores[0]
    high_count=0
    low_count=0
    min1=score
    max1=score
    for i in range(1,len(scores)):
        if scores[i]<min1:
            low_count+=1;
            min1=scores[i]
        elif scores[i]>max1:
            high_count+=1
            max1=scores[i]
    return(high_count,low_count)
